alias aliases='vim ~/.bash_profile'
alias e='explorer .'
alias ga='git add -A && git status'
alias gc='git checkout'
alias gcb='git checkout -b'
alias gcm='git commit -m'
alias gd='git diff'
alias gf='git fetch --prune'
alias gr='git reset HEAD^1'
alias grebase='git pull --rebase'
alias gs='git status'
alias ignore='git update-index --assume-unchaged'
alias ll='ls -l'
alias ls='ls -F --color=auto --show-control-chars'
alias pull='git pull'
alias push='git push'
alias pushf='git push -f'
alias squish='git rebase --interactive'
alias track='git update-index --no-assume-unchaged'
